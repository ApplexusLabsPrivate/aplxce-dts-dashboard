{
    "_version": "1.17.0",
    "sap.app": {
        "id": "com.applexus.dashboard",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:ovp",
            "version": "1.11.5",
            "toolsId": "65f42c73-90e9-4964-a222-53ed663ce7bf"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/aplxce/DTS_DEMO_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.71.59",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.f": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.suite.ui.generic.template": {},
                "sap.ovp": {},
                "sap.ui.rta": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.applexus.dashboard.i18n.i18n"
                }
            },
            "mainModel": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {},
            "routes": [],
            "targets": {}
        }
    },
    "sap.ovp": {
        "globalFilterModel": "mainModel",
        "globalFilterEntityType": "demoData",
        "containerLayout": "resizable",
        "enableLiveFilter": true,
        "considerAnalyticalParameters": false,
        "chartSettings": {
            "showDataLabel": true
        },
        "cards": {
            "DTS_card1": {
                "template": "sap.ovp.cards.charts.analytical",
                "model": "mainModel",
                "settings": {
                    "title": "Record Count",
                    "entitySet": "card1Set",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#card1",
                    "defaultSpan":{
                        "cols": 2,
                        "rows": 0
                    }
                }
            },
            "DTS_card2": {
                "template": "sap.ovp.cards.charts.analytical",
                "model": "mainModel",
                "settings": {
                    "title": "Record Count by Group",
                    "entitySet": "card2Set",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#card2",
                    "defaultSpan":{
                        "cols": 2,
                        "rows": 0
                    }
                    
                }
            },
            "DTS_card3": {
                "template": "sap.ovp.cards.charts.analytical",
                "model": "mainModel",
                "settings": {
                    "title": "Data Quality",
                    "entitySet": "card3Set",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#card3",
                    "defaultSpan":{
                        "cols": 2,
                        "rows": 0
                    }
                    
                }
            },
            "DTS_card4": {
                "template": "sap.ovp.cards.charts.analytical",
                "model": "mainModel",
                "settings": {
                    "title": "Volume",
                    "entitySet": "card4Set",
                    "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#card4",
                    "defaultSpan":{
                        "cols": 2,
                        "rows": 0
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    },
    "sap.ui.generic.app": {
        "settings": {}
    }
}